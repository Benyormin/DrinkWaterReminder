<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.beny.drinkwaterreminder">
    <uses-permission android:name="android.permission.SET_ALARM"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/drink_water_reminder"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/drink_water_reminder_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DrinkWaterReminder">
        <activity android:name=".Tips"></activity>
        <activity android:name=".List" />
        <activity android:name=".NotificationReminder" />
        <activity android:name=".CupsManager" />
        <activity android:name=".Pi" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".ReminderBroadcast" />
    </application>

</manifest>